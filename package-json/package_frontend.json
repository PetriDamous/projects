{
  "name": "western-front",
  "version": "1.0.0",
  "homepage": "/",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.6.6",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "classnames": "^2.3.1",
    "date-arithmetic": "^4.1.0",
    "graphql": "^15.8.0",
    "keycloak-js": "^6.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "prop-types": "^15.8.1",
    "react": "^16.14.0",
    "react-big-calendar": "^0.40.1",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.3.3",
    "react-scripts": "^5.0.1",
    "react-select": "^5.3.2",
    "reactstrap": "^8.9.0",
    "typescript": "^4.7.2"
  },
  "devDependencies": {
    "husky": "^8.0.1",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.2"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "jsxSingleQuote": true,
    "printWidth": 80,
    "quoteProps": "as-needed",
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "repository": {
    "type": "git",
    "url": "git://cfsr.sso.dcn:txw/txw-employees.git"
  },
  "scripts": {
    "build": "printenv | grep -e ^REACT_APP > .env.local && react-scripts build",
    "dev": "react-scripts start",
    "prepare": "husky install",
    "pretty-quick": "pretty-quick --staged --pattern \"**/*.*(js|jsx)\" --bail",
    "start": "npm install -g serve && serve -s build -p 8080"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
